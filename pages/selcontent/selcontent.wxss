/* pages/PunchTheClock/PunchTheClock.wxss */
.PunchView{
  width: 100%;
  height: 100%;
  background: #ffffff;
  padding: 20rpx;
  box-sizing: border-box;
}
.feedbackview{
  width: 710rpx;
  padding-bottom: 30rpx;
  box-sizing: border-box;
}

.submitform{
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.feedbacktxt{
  width: 680rpx;
  height: 188rpx;
  background: rgba(204,204,204,0.60);
  border:4rpx solid rgba(34,34,34,1);
  box-shadow: 0 2px 4px 0 rgba(153,153,153,0.25);
  border-radius: 15rpx;
  padding: 20rpx;
  margin-bottom: 60rpx;
  box-sizing: border-box;
  font-family: PingFang-SC-Regular;
  font-size: 24rpx;
  color: #666666;
  letter-spacing: 0;
}
.LearningExperience{
  padding-left: 20rpx; 
  padding-bottom: 10rpx;
}
.bigtxt{
  font-family: PingFang-SC-Regular;
  font-size: 36rpx;
  color: #333333;
  letter-spacing: 0;
}
.smalltxt{
  font-family: PingFang-SC-Regular;
  font-size: 26rpx;
  color: #2A2A2A;
  letter-spacing: 0;
  line-height: 25px;
}
.subBtn{
  width: 650rpx !important;
  height: 100rpx;
  position: relative;
  background: #EA2E44;
  color: #ffffff;
  border-radius: 50rpx;
  line-height: 62rpx;
  font-size: 30rpx;
}

.finally{
  width: 563rpx !important;
  height: 131rpx;
  
}

.LearningContent{
  margin-bottom: 30rpx;
}
.contentview{
  width: 100%;
  display: flex;
  justify-content: space-between;
  flex-wrap:wrap;
}
.contentviewlist{
  position: relative;
}
.contentviewlist-selected{
  border: 1rpx solid #000000;
}
.mianImg{
  width:330rpx;
  height: 142rpx;
}



.contentview2{
  display: flex;
  justify-content: space-between;
}

.contentviewlist2{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.contentviewlist2-selected{
  display: flex;
  flex-direction: column;
  align-items: center;
  border: 1rpx solid #000000;
}

.mianImg2{
  width: 82rpx;
  height: 82rpx;
  margin-bottom: 15rpx;
}
.txt{
  font-size: 30rpx;
  color: #222222;
}

.imagePathBox{
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,0.7);
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 10;
}

.baocun{
  display: block;
  width: 80% !important;
  height: 80rpx;
  padding: 0;
  line-height: 80rpx;
  text-align: center;
  position: fixed;
  bottom: 50rpx;
  left: 10%;
  background: #DC5829;
  color: #ffffff;
  font-size: 32rpx;
  border-radius: 44rpx;
}

.shengcheng{
  width: 80%;
  height: 80%;
  position: fixed;
  top: 50rpx;
  left: 50%;
  margin-left: -40%;
  z-index: 10;
}